[user]
	name = mueno-mba
    email = dummy@mail.com
[color]
    diff = auto
    status = auto
    branch = auto
    ui = true
[alias]
    st = !"tig status"
    ci = commit
    br = branch
    co = checkout
    df = diff
    dc = diff --cached
    lg = log -p
    lol = log --graph --decorate --pretty=oneline --abbrev-commit
    ls = ls-files
    unstage = reset HEAD --
    last = log -1 HEAD
    fix = !"git add -u && git commit -m 'fix' && git push"
[diff]
    tool = vimdiff
[url "https://"]
    insteadOf = git://
[gist.github.com]
    user = m-ueno
[github]
    user = m-ueno
[push]
    default = matching
[core]
    excludesfile = /home/masaru/.gitignore
[credential]
	helper = cache --timeout=86400

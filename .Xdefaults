urxvt*background:black
urxvt*foreground:gray80
urxvt*cursorColor:pink
urxvt*scrollBar: False
urxvt*transparent:true
urxvt*fading:40
urxvt*shading:20
!urxft*fadeColor:black
!URxvt.imFont: -*-medium-r-*-20-*
URxvt.preeditType: OverTheSpot
!URxvt.lineSpace: 2

!! Perl extensions (ArchWiki#rxvt-unicode)
! Clickable URLs
! urxvt.perl-ext-common: default, matcher
! urxvt.urlLauncher: /usr/bin/chromium
! urxvt.matcher.button: 1

!! Yankable URLs (No mouse)
! pacman -S urxvt-url-select (community)
! Note: This extension replaces the Clickable URLs extension mentioned above, so matcher can be removed from the URxvt.perl-ext list.
! <<<Usage>>>
! M-u Enter selection mode. The last URL on your screen will be selected. You can repeat Alt+u to select the next upward URL.
! k Select next upward URL
! j Select next downward URL
! Return Open selected URL in browser and quit selection mode
! o Open selected URL in browser without quitting selection mode
! y Copy (yank) selected URL and quit selection mode
! Esc Cancel URL selection mode
urxvt.perl-ext: default,url-select,clipboard
urxvt.keysym.M-u: perl:url-select:select_next
urxvt.urlLauncher: /usr/bin/opera
urxvt.underlineURLs: true

!urxvt*font:xft:IPAGothic,xft:Meiryo
!urxvt*font:xft:M+2VM+IPAG circle:size=12
